%li.main-page__header__nav-box__right-box__list__item
  = link_to users_path, class: 'main-page__header__nav-box__right-box__list__item__root' do
    %figure.main-page__header__nav-box__right-box__list__item__root__user-icon
      .main-page__header__nav-box__right-box__list__item__root__user-icon__image-wrap
        = image_tag 'member_photo_noimage_thumb.png', class: "main-page__header__nav-box__right-box__list__item__root__user-icon__image"
    %span.main-page__header__nav-box__right-box__list__item__root__text
      マイページ
  .main-page__header__nav-box__right-box__list__item__item-box.main-page__header__nav-box__right-box__list__item__item-box--mypage
    .main-page__header__nav-box__right-box__list__item__item-box--mypage__user-state
      %figure.main-page__header__nav-box__right-box__list__item__item-box--mypage__user-figure
        .main-page__header__nav-box__right-box__list__item__item-box--mypage__user-figure__image-wrap
          = image_tag 'member_photo_noimage_thumb.png', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__user-figure__image'
        %figcaption.main-page__header__nav-box__right-box__list__item__item-box--mypage__user-figure__name
          = current_user.nickname
      %ul.main-page__header__nav-box__right-box__list__item__item-box--mypage__review-listing
        %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__review-listing__item
          = link_to '評価： 0', '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__review-listing__link'
        %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__review-listing__item
          = link_to '出品数: 0', '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__review-listing__link'
    %ul.main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__link' do
          .main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__text
            売上金
          .main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__value
            ¥ 0
            = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__icon-arrow-right'
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__link' do
          .main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__text
            ポイント
          .main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__value
            P 0
            = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__sales-point__item__icon-arrow-right'
    %ul.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__text' do
          いいね！一覧
          = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__icon-arrow-right'
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item
        = link_to sell_items_list_users_path, class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__text' do
          出品した商品 - 出品中
          = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__icon-arrow-right'
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__text' do
          出品した商品 - 取引中
          = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__icon-arrow-right'
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__text' do
          出品した商品 - 売却済み
          = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__icon-arrow-right'
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__text' do
          購入した商品 - 取引中
          = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__icon-arrow-right'
      %li.main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item
        = link_to '', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__text' do
          購入した商品 - 過去の取引
          = fa_icon 'angle-right', class: 'main-page__header__nav-box__right-box__list__item__item-box--mypage__nav-list__item__icon-arrow-right'
